    "#creating dictonary to store the data\n"
    "dic={'designation':[],'comapany':[],'employeecount':[],'followers':[],'level':[],'totalapplicants':[],'location':[],'industry':[]} "
    "time.sleep(7)  #Sleep time to wait for page to load \n"
   ]
